function is_collision = is_in_obstacle(P,O)
%IS_IN_OBSTACLE この関数の概要をここに記述
%   詳細説明をここに記述

margin=0.05;

is_collision = (P(1) >= O(1)-margin) && (P(1) <= O(2)+margin) && ... % X軸方向の判定
                   (P(2) >= O(3)-margin) && (P(2) <= O(4)+mar) && ... % Y軸方向の判定
                   (P(3) >= O(5)) && (P(3) <= O(6));       % Z軸方向の判定
end